{
  "name": "mangaviewer",
  "version": "1.0.0",
  "description": "A Manga Viewer",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "rebuild": "./node_modules/.bin/electron-rebuild -f -w sharp",
    "dist": "electron-packager . --platform=win32 --overwrite --arch=x64 MangaView"
  },
  "author": "Royel Consoro",
  "license": "MIT",
  "dependencies": {
    "ffmpeg-static": "^2.4.0",
    "ffprobe-static": "^3.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.0",
    "node-stream-zip": "^1.7.0",
    "rcunrar": "file:../local_modules/rcunrar",
    "sequelize": "^4.39.1",
    "sharp": "^0.20.8",
    "sqlite3": "^4.0.2",
    "win-explorer": "file:../local_modules/win-explorer",
    "zip-local": "^0.3.4"
  },
  "build": {
    "appId": "rc.manga.download",
    "win": {
      "target": "nsis"
    }
  },
  "devDependencies": {
    "electron": "^3.0.12",
    "electron-rebuild": "^1.8.2",
    "node-abi": "^2.4.4"
  }
